            <TextBox HorizontalAlignment="Left" Height="23" Margin="84,167,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
            <Label Content="Prénom :" HorizontalAlignment="Left" Margin="220,167,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.032,0.233" Width="57" Height="23"/>
