                        <Grid Background="#FFE5E5E5" Margin="0,0,134,658"/>
                    </Grid>
                </TabItem>
                <TabItem x:Name="tabItem_createVacation" Header="Créer vacation" Margin="10,0,-10,0">
                    <Grid Background="White" Margin="-12,0,-1,-12">
