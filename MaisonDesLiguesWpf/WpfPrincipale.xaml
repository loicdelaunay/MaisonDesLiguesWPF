                        <TextBox x:Name="TextboxVille_Copy2" HorizontalAlignment="Left" Height="23" Margin="383,227,-60,-102" TextWrapping="Wrap" VerticalAlignment="Top" Width="180" RenderTransformOrigin="0.527,0.633"/>
