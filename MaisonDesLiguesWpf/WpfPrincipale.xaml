                        <xctk:DateTimePicker x:Name="datePicker_HeureFinVacation" Format="Custom" FormatString="dd/MM/yyyy hh:mm" TimeFormat="Custom" TimeFormatString="HH:mm" HorizontalAlignment="Left" Margin="163,244,0,0" VerticalAlignment="Top" Width="512"/>
                        <Button x:Name="Btn_CreateVacation" Content="Créer vacation" HorizontalAlignment="Left" Margin="551,310,0,0" VerticalAlignment="Top" Width="124" Click="Btn_CreateVacation_Click"/>
                        <Button Content="Rafraichir" x:Name="btn_rafraichirAtelier" HorizontalAlignment="Left" Margin="695,66,0,0" VerticalAlignment="Top" Width="87" Height="25" Click="btn_rafraichirAtelier_Click"/>
                    </Grid>
                </TabItem>
                <TabItem x:Name="tabItem_createTheme" Header="Créer thème" Margin="10,0,-10,0">
                    <Grid Background="White" Margin="-12,2,2,-17">
                        <ComboBox x:Name="ComboBox_ThemeChoisirAtelier" HorizontalAlignment="Left" Margin="163,70,0,0" VerticalAlignment="Top" Width="512" Height="25" Text="Choisir un atelier"/>
                        <Label Content="Choisir un atelier : " HorizontalAlignment="Left" VerticalAlignment="Top" Margin="28,69,0,0"/>
